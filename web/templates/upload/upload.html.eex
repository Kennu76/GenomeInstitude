<h1> Upload here </h1>
<!-- Here is the upload form-->
<%= form_for @conn, upload_path(@conn, :upload), [as: "upload", multipart: :true], fn f -> %>  
  <%= file_input f, :file, accept: "*" %>
  <button class="btn btn-primary" type="submit">Upload</button>
<% end %>  
<!-- This button sends the user to the Database site-->
<span><%= button("Go to Database", to: "/datas", method: "get", class: "btn") %></span>